proc _poc_reanalyze {} {
	set returnCode [exec -ignorestderr py -3 ../../py/PoC.py -d vsim PoC.sort.sortnet.BitonicSort --analyze --elaborate]
	puts "ReturnCode: $returnCode"
}

proc poc_recompile {} {
	_poc_reanalyze
	restart -force
	puts "DONE"
}

proc poc_rerun {} {
	restart -force
	# if xxx exists
